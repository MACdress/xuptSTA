<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>01-serverletCyle</display-name>
  <servlet>
  		<servlet-name>some-servlet</servlet-name>
		<servlet-class>com.xupt.serverletConfig.ServerletConfig</servlet-class>
		<init-param>
				<param-name>gaopan</param-name>
				<param-value>learning</param-value>
		</init-param>
		<init-param>
				<param-name>gaopan2</param-name>
				<param-value>successful</param-value>
		</init-param>
  </servlet>
  <servlet-mapping>
  		<servlet-name>some-servlet</servlet-name>
  		<url-pattern>/some</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>